* {
  border: #7d6a40;
  border-color: #7d6a40;
  border-radius: 15px;

  padding: 10px;

  margin: 10px;
}

window {
  background-color: #d3c6aa;

  color: #2d353b;

  width: 550px;
}

inputbar {
  background-color: #a7c080;

  border-color: #a7c080;

  color: #2d353b;

  children: [entry, overlay, case-indicator];
}

listview {
  background-color: #d3c6aa;

  border-color: #d3c6aa;

  dynamic: true;

  margin: 0px;

  lines: 5;

  padding: 0px;

  scrollbar: false;
}